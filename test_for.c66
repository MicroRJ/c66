	if(i<5)continue;
	break;
}
eq(5,i);
for(i=0;i<10;i=i+1){
	if(i>=5)break;
}
eq(5,i);
for(i=0;i<10;i=i+1);
eq(10,i);
for(i=0;i<=9;i=i+1);
eq(10,i);
for(i=10;i>0;i=i-1);
